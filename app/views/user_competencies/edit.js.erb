var edit_form = $("<%= j render 'form_user_competency', user_competency: @user_competency, experience: @experience %>");
$('.modal-content').empty().append(edit_form);
<% @user_competency.competencies.each do |badge| %>
  $(".modal-content #user_competency_competency_ids_<%= badge.id %>").prop('checked', true);
  $(".modal-content #user_competency_competency_ids_<%= badge.id %>").parent().addClass('active');
<% end %>
$('#myModal').modal('show');
$('.modal-content #ex8').slider({
  tooltip: 'always'
});
$(".modal-content #ex8").on("slide", function(slideEvt) {
  $(".modal-content #user_competency_level").val(slideEvt.value);
});
$('.check_boxes').change(function(){
  $(this).parent().toggleClass('active');
});
