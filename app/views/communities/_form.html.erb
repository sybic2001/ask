<%= simple_form_for community do |f| %>
  <%= f.input :name, label: "Nom de la communauté" %>
  <%= f.input :description, label: "Description" %>
  <%= f.input :url, label: "Site internet de la communauté" %>
  <%= f.input :photo, as: :attachinary %>
  <%= f.submit :button, class: 'btn btn-primary' %>
<% end %>
